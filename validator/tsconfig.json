{
    "name": "@regionstats/validator",
    "private": true,
    "compileOnSave": false,
    "include": [
        "src/**/*.ts"
    ],
    "compilerOptions": {
        "outDir": "./dist",
        "declaration": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": [
            "es2015",
            "dom"
        ],
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es5",
        "watch": false
    }
}